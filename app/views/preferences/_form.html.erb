<%= form_for @preference do |f| %>
  <div class="field">
    <%= f.label :age_range %><br />
    <%= f.select :age_range, ['Any','18-24','25-30','31-36','37-45']%>
  </div>

  <div class="field">
    <%= f.label :gender %><br />
    <%= f.select :gender, ['Any','He/Him', 'She/Her'] %>
  </div>

  <div class="actions">
    <%= f.submit "Save Preferences" %>
  </div>


<% end %>
